{
	"name": "feft",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "nodemon",
		"type-check": "tsc --noEmit",
		"lint": "biome lint --error-on-warnings ./src",
		"format": "biome format ./src --fix",
		"clean": "biome check ./src --write",
		"release": "tsc --noEmit && rm -rf dist && pnpm run build",
		"build": "swc --strip-leading-paths -d dist src",
		"db:push": "pnpm release && drizzle-kit push",
		"start:console": "pnpm release && node --env-file=.env dist/console.js"
	},
	"dependencies": {
		"@fastify/autoload": "^6.3.0",
		"@fastify/cors": "^11.0.1",
		"@fastify/sensible": "^6.0.3",
		"@fastify/swagger": "^9.5.0",
		"@fastify/swagger-ui": "^5.2.2",
		"@mysten/sui": "^1.27.0",
		"@node-rs/jsonwebtoken": "^0.5.9",
		"drizzle-orm": "^0.41.0",
		"drizzle-zod": "^0.7.1",
		"effect": "^3.14.8",
		"fastify": "^5.3.0",
		"fastify-plugin": "^5.0.1",
		"fastify-type-provider-zod": "^4.0.2",
		"ioredis": "^5.6.1",
		"luxon": "^3.6.1",
		"pg": "^8.14.1",
		"randomstring": "^1.3.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@swc/cli": "^0.7.2",
		"@swc/core": "^1.11.21",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/luxon": "^3.6.2",
		"@types/node": "^22.14.1",
		"@types/pg": "^8.11.13",
		"@types/randomstring": "^1.3.0",
		"drizzle-kit": "^0.30.6",
		"nodemon": "^3.1.9",
		"typescript": "^5.8.3"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"@biomejs/biome",
			"@swc/core",
			"esbuild"
		]
	}
}
